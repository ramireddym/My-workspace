package dinesh;

public class string {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		StringBuffer sb=new StringBuffer("Hello");
		sb.append("SQL");
		System.out.println(sb);
		sb.insert(1,"java");
		System.out.println(sb);
		sb.replace(1,3,".net");
		System.out.println(sb);
		sb.delete(1,3);
		System.out.println(sb);
		sb.reverse();
		System.out.println(sb);
				
	}

	
}
